{
  "manifest_version": 2,
  "name": "Eliminate Keyword Link",
  "short_name": "KeywordLink",
  "version": "1.0.0",
  "description": "はてなブログに追加される鬱陶しいキーワードリンクを除去します！",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.hatenablog.com/*"],
      "js": ["jquery.min.js", "main.js"]
    }
  ]
}
